{
    "name": "combo",
    "version": "1.0.0",
    "description": "combo",
    "types": "./dist/types.d.ts",
    "private": true,
    "scripts": {
        "watch": "parcel watch",
        "build": "parcel build --target combo",
        "test": "jest"
    },
    "devDependencies": {
        "bootstrap": "5.1",
        "@types/bootstrap": "5.1",
        "@popperjs/core": "2.10",
        "parcel": "2.0"
    },
    "alias": {
        "bootstrap": {
            "global": "bootstrap"
        }
    },
    "browserslist": [
        "> 0.5%, last 2 versions, not dead"
    ],
    "combo": "./dist/combo.min.js",
    "targets": {
        "combo": {
            "source": "./combo.js",
            "context": "browser",
            "outputFormat": "global",
            "includeNodeModules": true,
            "isLibrary": false
        }
    },
    "dependencies": {
        "nanoid": "3.1"
    },
    "peerDependencies": {
        "@popperjs/core": "2.10",
        "bootstrap": "5.1"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "../../conf/JestExtend.js"
        ],
        "testEnvironment": "jsdom",
        "testEnvironmentOptions": {
            "userAgent": "Agent/007"
        },
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/_test/js/*.test.[jt]s?(x)"
        ]
    }
}
