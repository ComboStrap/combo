{
  "name": "combo",
  "version": "1.0.0",
  "description": "combo",
  "type": "module",
  "types": "./dist/combo.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/combo.js",
  "module": "./dist/combo.es.js",
  "exports": {
    ".": {
      "import": "./dist/combo.es.js",
      "require": "./dist/combo.js"
    }
  },
  "private": true,
  "scripts": {
    "preview": "vite preview --outDir .",
    "build": "tsc && vite build && yarn types",
    "deploy": "vitest run && yarn build && cp dist/combo.js ../../../resources/library/combo/combo.min.js || cp dist/combo.js ../../../resources/library/combo/combo.min.js",
    "test": "vitest watch",
    "types": "tsc --project tsconfig.types.json",
    "coverage": "vitest run --coverage",
    "watch": "vite build --watch"
  },
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "dependencies": {
    "@popperjs/core": "2.10",
    "@types/bootstrap": "5.1",
    "bootstrap": "5.1",
    "nanoid": "3.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16"
  }
}
