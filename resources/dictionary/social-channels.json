{
    "facebook": {
        "share": {
            "web": "https://facebook.com/sharer/sharer.php?u=$url",
            "popup": "Share this page with Facebook",
            "doc": "https://developers.facebook.com/docs/plugins/share-button/"
        },
        "follow": {
            "web": "https://facebook/$handle"
        },
        "colors": {
            "primary": "#3b5998",
            "secondary": "#2d4373"
        },
        "icons": {
            "outline": "iconoir:facebook",
            "outline-circle": "et:facebook",
            "solid": "eva:facebook-fill",
            "solid-circle": "bi:facebook"
        }
    },
    "twitter": {
        "share": {
            "web": "https://twitter.com/intent/tweet/?url=$url&text=$title%0A$description%0A&via=$via",
            "popup": "Share this page with Twitter"
        },
        "follow": {
            "web": "https://twitter.com/intent/follow?screen_name=$handle",
            "popup": "Follow us on Twitter",
            "doc": "https://developer.twitter.com/en/docs/twitter-for-websites/follow-button/guides/web-intent-follow-button"
        },
        "colors": {
            "primary": "#55acee",
            "secondary": "#2795e9"
        },
        "icons": {
            "outline": "uit:twitter-alt",
            "outline-circle": "et:twitter",
            "solid": "mdi:twitter",
            "solid-circle": "ant-design:twitter-circle-filled"
        }
    },
    "tumblr": {
        "share": {
            "web": "https://www.tumblr.com/widgets/share/tool?posttype=link&title=$title&caption=$description&canonicalUrl=$url&content=$url",
            "popup": "Share this page with Tumblr"
        },
        "colors": {
            "primary": "#35465C",
            "secondary": "#222d3c"
        },
        "icons": {
            "solid": "carbon:logo-tumblr",
            "solid-circle": "entypo-social:tumblr-with-circle",
            "outline": "uit:tumblr-alt",
            "outline-circle": "et:tumblr"
        }
    },
    "email": {
        "share": {
            "web": "mailto:?subject=$title&body=$description%20$url",
            "popup": "Share this page via Email"
        },
        "colors": {
            "primary": "#777777",
            "secondary": "#5e5e5e"
        }
    },
    "whatsapp": {
