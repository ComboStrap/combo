{
    "facebook": {
        "endpoint": "https://facebook.com/sharer/sharer.php",
        "queries": {
            "url": "u"
        },
        "icons": {
            "outline": "uit:facebook-f",
            "outline-circle": "et:facebook",
            "solid": "eva:facebook-fill",
            "solid-circle": "bi:facebook"
        }
    },
    "twitter": {
        "endpoint": "https://twitter.com/intent/tweet/",
        "queries": {
            "url": "url",
            "text": "text"
        },
        "icons": {
            "outline": "uit:twitter-alt",
            "outline-circle": "et:twitter",
            "solid": "mdi-twitter",
            "solid-circle": "ant-design:twitter-circle-filled"
        }
    },
    "tumblr": {
        "endpoint": "https://www.tumblr.com/widgets/share/tool?posttype=link",
        "queries": {
            "text": [
                "title",
                "caption"
            ],
            "url": [
                "content",
                "canonicalUrl"
            ]
        },
        "icons": {
            "solid": "carbon:logo-tumblr",
            "solid-circle": "entypo-social:tumblr-with-circle",
            "outline": "uit:tumblr-alt",
            "outline-circle": "et:tumblr"
        }
    },
    "pinterest": {
        "endpoint": "https://pinterest.com/pin/create/button/",
        "queries": {
            "url": [
                "url",
                "media"
            ],
            "text": "description"
        },
        "icons": {
            "solid": "entypo-social:pinterest",
            "solid-circle": "bi:pinterest",
            "outline": "simple-line-icons:social-pinterest",
            "outline-circle": "icomoon-free:pinterest2"
        }
    },
    "linkedin": {
        "endpoint": " https://www.linkedin.com/shareArticle?mini=true",
        "queries": {
            "url": [
                "url",
                "source"
            ],
            "text": [
                "title",
                "summary"
            ]
        },
        "icons": {
            "solid": "entypo-social:linkedin",
            "solid-circle": "entypo-social:linkedin-with-circle",
            "outline": "uit:linkedin-alt",
            "outline-circle": "et:linkedin"
        }
    },
    "reddit": {
        "endpoint": "https://reddit.com/submit/?resubmit=true",
        "queries": {
            "url": "url",
            "text": "title"
        },
        "icons": {
            "solid": "dashicons:reddit",
            "solid-circle": "bi:reddit",
            "outline": "simple-line-icons:social-reddit",
            "outline-circle": "simple-line-icons:social-reddit"
        }
    },
    "email": "mailto:?subject="
    +
    text
    +
    "&body="
    +
    url,
    "xing": "https://www.xing.com/app/user?op=share;url="
    +
    url
    +
    ";title="
    +
    text,
    "whatsapp": "whatsapp://send?text="
    +
    text
    +
    "%20"
    +
    url,
    "hackernews": "https://news.ycombinator.com/submitlink?u="
    +
    url
    +
    "&t="
    +
    text,
    "vk": "http://vk.com/share.php?title="
    +
    text
    +
    "&url="
    +
    url,
    "telegram": "https://telegram.me/share/url?text="
    +
    text
    +
    "&url="
    +
    url
}
