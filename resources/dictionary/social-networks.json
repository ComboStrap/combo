{
    "facebook": {
        "endpoint": "https://facebook.com/sharer/sharer.php",
        "queries": {
            "url": "u"
        },
        "icons": {
            "outline": "uit:facebook-f",
            "outline-circle": "et:facebook",
            "solid": "eva:facebook-fill",
            "solid-circle": "bi:facebook"
        }
    },
    "twitter": {
        "endpoint": "https://twitter.com/intent/tweet/",
        "queries": {
            "url": "url",
            "text": "text"
        },
        "icons": {
            "outline": "uit:twitter-alt",
            "outline-circle": "et:twitter",
            "solid": "mdi-twitter",
            "solid-circle": "ant-design:twitter-circle-filled"
        }
    },
    "tumblr": {
        "endpoint": "https://www.tumblr.com/widgets/share/tool?posttype=link",
        "queries": {
            "text": [
                "title",
                "caption"
            ],
            "url": [
                "content",
                "canonicalUrl"
            ]
        },
        "icons": {
            "solid": "mdi-twitter",
            "solid-circle": "ant-design:twitter-circle-filled",
            "outline": "uit:twitter-alt",
            "outline-circle": "et:twitter"
        }
    },
    "pinterest": "https://pinterest.com/pin/create/button/?url=$url&media=$url&description=$text",
    "linkedin": "https://www.linkedin.com/shareArticle?mini=true&url="
    +
    url
    +
    "&title="
    +
    text
    +
    "&summary="
    +
    text
    +
    "&source="
    +
    url,
    "reddit": "https://reddit.com/submit/?url="
    +
    url
    +
    "&resubmit=true&title="
    +
    text,
    "email": "mailto:?subject="
    +
    text
    +
    "&body="
    +
    url,
    "xing": "https://www.xing.com/app/user?op=share;url="
    +
    url
    +
    ";title="
    +
    text,
    "whatsapp": "whatsapp://send?text="
    +
    text
    +
    "%20"
    +
    url,
    "hackernews": "https://news.ycombinator.com/submitlink?u="
    +
    url
    +
    "&t="
    +
    text,
    "vk": "http://vk.com/share.php?title="
    +
    text
    +
    "&url="
    +
    url,
    "telegram": "https://telegram.me/share/url?text="
    +
    text
    +
    "&url="
    +
    url
}
